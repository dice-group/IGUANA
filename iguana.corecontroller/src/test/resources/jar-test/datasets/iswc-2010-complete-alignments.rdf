    <dc:title>dbrec - Music Recommendations Using DBpedia</dc:title>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/431/authorList"/>
    <cofunc:keyword>semantic web applications</cofunc:keyword>
