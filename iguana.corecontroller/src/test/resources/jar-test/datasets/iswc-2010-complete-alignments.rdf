    <dc:title>Optimizing Enterprise-scale OWL 2 RL Reasoning in a Relational Database System</dc:title>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2010/proceedings"/>
