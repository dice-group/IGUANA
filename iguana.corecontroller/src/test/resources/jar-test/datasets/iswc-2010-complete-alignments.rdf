  <rdf:Description rdf:about="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/499">
    <cofunc:title>RightField: Embedding Ontology Term Selection into Spreadsheets for the Annotation of Biological Data</cofunc:title>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/olga-krebs"/>
    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
