    <rdf:type rdf:resource="http://purl.org/spar/fabio/ProceedingsPaper"/>
    <foaf:maker rdf:resource="https://w3id.org/scholarlydata/person/william-cohen"/>
    <cofunc:hasAuthorList rdf:resource="https://w3id.org/scholarlydata/inproceedings/iswc2010/paper/319/authorList"/>
