    <dc:title>SPARQL Views: A Visual SPARQL Query Builder for Drupal</dc:title>
    <cofunc:title>SPARQL Views: A Visual SPARQL Query Builder for Drupal</cofunc:title>
    <dc:subject>User Interfaces</dc:subject>
    <cofunc:isPartOf rdf:resource="https://w3id.org/scholarlydata/conference/iswc/2010/poster-demo-proceedings"/>
